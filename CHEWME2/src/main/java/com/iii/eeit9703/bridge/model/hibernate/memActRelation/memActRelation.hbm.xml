<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.iii.eeit9703.bridge.model.MemActRelationVO" table="join_activitys" >
        <composite-id>
            <!-- key-property标签表示哪一些属性对应复合主键 -->
           <!--  <key-property name="clubId" column="clubID" type="int"></key-property>
            <key-property name="memId" column="memID" type="int"></key-property> -->           
            <key-property name="actId" column="actID"></key-property>
            <key-property name="memId" column="memID" ></key-property>           
        </composite-id>
                
        <property name="actId" column="actId" />
        <property name="memId" column="memId" />
        
         <property name="date" column="joinTime" />
        
    </class>
</hibernate-mapping>




