<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.crawler.model.AttrVO" table="Attractions">
		<id name="attractionID" column="attractionID">
			<generator class="identity" />

		</id>
		<property name="name" column="name" />
		<property name="county" column="county" />
		<property name="type" column="type" />
		<property name="address" column="address" />
		<property name="tel" column="tel" />
		<property name="intro" column="intro" />
		
	</class>
</hibernate-mapping>