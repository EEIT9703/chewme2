<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.crawler.model.ArticleVO" table="article_share">
		<id name="articleID" column="articleID">
			<generator class="identity" />
		</id>
		<property name="mId" column="mId" />
		<property name="attractionID" column="attractionID" />
		<property name="contents" column="contents" />
	</class>
</hibernate-mapping>