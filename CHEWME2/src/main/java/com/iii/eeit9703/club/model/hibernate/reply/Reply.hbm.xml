<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.club.model.ReplyVO" table="replys">

		<id name="reqlyId" column="reqlyId">
			<generator class="identity" />
		</id>

		<property name="qId" column="qId" />
		<property name="replyContent" column="content" />
		<property name="replyerId" column="replyerId" />
		<property name="replyDate" column="replyDate" />
		<property name="readed" column="readed" />
	</class>
</hibernate-mapping>

