<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.iii.eeit9703.club.model.QuestionVO" table="questions">

		<id name="questionId" column="questionId">
			<generator class="identity" />
		</id>
		
		<property name="clubId" column="clubId" />
		<property name="qcontent" column="qcontent" />
		<property name="askerId" column="askerId" />
		<property name="readed" column="readed"/>
	</class>
</hibernate-mapping>

